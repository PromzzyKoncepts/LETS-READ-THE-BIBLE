{
    "headers": [
      {
        "source": "/app/api/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "POST, GET, OPTIONS" },
          { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
        ]
      }
    ],
    "redirects": [
    {
      "source": "/",
      "destination": "https://letsreadthebible.club",
      "permanent": false
    }
  ]

  }